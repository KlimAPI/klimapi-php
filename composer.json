{
    "name": "klimapi/php",
    "version": "2.0.14",
    "description": "This API offers you the possibility to calculate and offset emissions, create checkout links, get statistics and much more.",
    "keywords": [
        "php",
        "sdk",
        "rest",
        "klimapi",
        "api"
    ],
    "homepage": "https://github.com/KlimAPI/klimapi-php",
    "license": "MIT",
    "authors": [
        {
            "name": "KlimAPI",
            "homepage": "https://klimapi.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "KlimAPI\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "KlimAPI\\Test\\" : "test/" }
    }
}
